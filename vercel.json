{
  "version": 2,
 
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
	

  ]
}

{
  "routes": [
    { "handle": "files" },
    { "src": ".*", "dest": "/index.html" }
  ]
}

{
  "functions": {
    "api/**": { "memory": 1024, "maxDuration": 10 }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}